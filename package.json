{
  "name": "better-piazza",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc mocha --file test/test.js --timeout 6666 --require babel-polyfill --require babel-register --exit && nyc report --reporter=html --reporter=text-lcov | coveralls",
    "server": "cross-env NODE_ENV=development nodemon --exec babel-node server/server.js",
    "build-server": "cross-env NODE_ENV=production babel -d ./build ./server -s",
    "start":"node ./bin/www"
  },
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "~1.18.2",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "express-session": "^1.15.6",
    "mongodb": "^3.0.7",
    "mongoose": "^5.0.17",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-react": "^7.7.0",
    "mocha": "^5.1.1",
    "mocha-eslint": "^4.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.17.4",
    "nyc": "^11.7.1"
  }
}
